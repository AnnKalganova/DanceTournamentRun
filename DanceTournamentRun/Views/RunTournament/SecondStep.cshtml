
@{ 
    var group = ViewBag.currentGroup as Group;
}
@model Tournament
<div class="m-3">
    <h2>Шаг 2</h2>
    <h3>@group.Name</h3>

    <div id="grCompetPart"></div>
</div>

<script>
    $.get('@Url.Action("GetGroupCompetData", "RunTournament")', { group: @group}, function (content) {
        $("#grCompetPart").html(content);
        });
</script>